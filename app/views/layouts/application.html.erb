<!doctype html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->
<head>

	<!-- Basic Page Needs
  	================================================== -->
	<meta charset="utf-8" />
	<title>Telepath Dashboard</title>
	<meta name="description" content="">
	<meta name="author" content="">
	<!--[if lt IE 9]> 
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->
	
	<!-- Mobile Specific Metas
  	================================================== -->
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" /> 
	
	<!-- CSS
  	================================================== -->
  	<%= stylesheet_link_tag "styles" %>
	<%= stylesheet_link_tag "jsDatePick_ltr.min.css" %>
	
	
	<!-- Favicons
	================================================== -->
	<link rel="shortcut icon" href="/images/favicon.ico" />
	<link rel="apple-touch-icon" href="/images/apple-touch-icon.png" />
	<link rel="apple-touch-icon" sizes="72x72"  href="/images/apple-touch-icon-72x72.png" />
	<link rel="apple-touch-icon" sizes="114x114" href="images/apple-touch-icon-114x114.png" />
	
	
  	<%= javascript_include_tag "jquery-1.6.2.min.js" %>
  	<%= javascript_include_tag "jquery.tmpl.min" %>
  	<%= javascript_include_tag "jsDatePick.min.1.3.js" %>
  	<%= csrf_meta_tag %>
  	
  	<!--Load the AJAX API-->
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
  	<%= yield :head %>

</head>

<body>
  
<!-- Super Container for entire site -->
<div class="super-container full-width" id="section-header">

	<!-- 960 Container -->
	<div class="container">			
		
		<!-- Header -->
		<header>
		<div class="sixteen columns">
			
			<!-- Branding -->
			<div class="six columns alpha">
				<a href="http://labmed:3000">
					<h1 id="logo" style="margin-top: 40px; margin-bottom: 30px;">
						<%= image_tag "logo.png" %>
					</h1>
				</a>
			</div>
			<!-- /End Branding -->
			
			<hr class="remove-bottom"/>
		</div>
		
		
		<div class="sixteen columns" id="menu"><!-- Menu -->
			
			<div class="twelve columns alpha navigation">
				
				<!-- Default Superfish Nav (add/remove "light" class to toggle visual styling) -->
				<ul class="sf-menu light">
					<li><a href="http://labmed:3000"><strong>Home</strong></a></li>
					<li><a href="http://labmed/ed/index.html"><strong>ED Tracker</strong> <span>Emergency Department Tracker</span></a></li>
				</ul>
				<!-- /End Default Superfish Nav-->
				
				<!-- Responsive Nav - Displays when screen gets small enough -->
				<form id="responsive-nav" action="insert_action" method="post">
					<select class="chzn-select">
						<option value="index.html">Home</option>
						<option value="/ed/index.html">ED Tracker</option>
	
					</select>
				</form>
				<!-- /End Responsive Nav -->
		</div>		
			<hr class="remove-top"/>
		</div>	<!-- /End Menu -->
		 
		
		
		</header>
		<!-- /End Header -->
	
	</div>
	<div class="container">	
		<div class="sixteen columns clearfix">
		
			<div class="eight columns alpha">
			</div>
			
			<div class="eight columns omega">
			</div>
		
		</div>
	
	</div>
	<%= yield %>
</div>
<!-- End SuperContainer -->
  		
<!-- Super Container Footer-->
<div class="super-container full-width" id="section-footer">		
	<!-- 960 Container -->
	<div class="container">
		<!-- footer -->
		<footer>
		<div class="sixteen columns">
			<p>Directorate of Laboratory Medicine, for help and support contact Craig Webster</p>
		</div>
		</footer>
		<!-- /End Footer -->
	</div>
	<!-- /End 960 Container -->
</div>
<!-- /End Super Container Footer -->
</body>
</html>
